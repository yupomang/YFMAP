  <select id="selectGjgrmx" parameterClass="com.yondervision.yfmap.dto.GjGrmx" resultMap="gjgrmx_GjgrmxResult">
    SELECT
    	gj_grmx.*,gg_flm.mc as jymmc,case when gj_grmx.zhlx='01' then '公积金账户' when gj_grmx.zhlx='02' then '按月补贴账户' when gj_grmx.zhlx='03' then '货币补贴账户' end as zhlxm
