    <dynamic prepend="where" >
	  <isNotEmpty prepend="and" property="grsb">
	  	gj_grmx.grsb =#grsb:CHAR#
	  </isNotEmpty>
	  <isNotEmpty prepend="and" property="zhlx">
	   gj_grmx.ZHLX = #zhlx:CHAR#
	  </isNotEmpty>
    </dynamic>
   	AND gj_grmx.jym = gg_flm.jym order by jyrq desc
